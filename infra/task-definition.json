{
    "family": "asimov",
    "containerDefinitions": [
        {
            "name": "asimov-container",
            "image": "476300129823.dkr.ecr.eu-central-1.amazonaws.com/asimov:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": []
        }
    ],
    "executionRoleArn": "arn:aws:iam::476300129823:role/ecsTaskExecutionRole",
    "networkMode": "bridge",
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "256",
    "memory": "512"
}
